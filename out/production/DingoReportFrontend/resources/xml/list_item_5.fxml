<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root stylesheets="/ui/res/css/styles.css" type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

    <Pane fx:id="parent" style="-fx-background-color: #E3E3E3;-fx-background-radius: 15 15 15 15;">
        <VBox fx:id="vbox" prefWidth="${parent.width}" spacing="4">
            <padding><Insets bottom="8" left="8" right="8" top="8" /></padding>
            <Label fx:id="teamId" text="Team #">
                <font>
                    <Font name="System Bold" size="16.0" />
                </font>
            </Label>
            <Label fx:id="reports" text="Assigned Reports: #">
                <font>
                    <Font name="Helvetica" size="16.0" />
                </font>
            </Label>
        </VBox>
    </Pane>


</fx:root>
