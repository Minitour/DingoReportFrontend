<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import ui.UIView?>
<UIView fx:id="root" xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        stylesheets="@../../ui/res/css/styles.css"
        prefHeight="400.0" prefWidth="600.0">
    <HBox spacing="8"
          alignment="CENTER"
          styleClass="container">
        <padding><Insets top="8" left="8" right="8" bottom="8"></Insets></padding>
        <ListView fx:id="from" HBox.hgrow="NEVER" prefHeight="300"/>
        <JFXButton text="Assign" fx:id="assign" styleClass="button-raised" prefWidth="60" minWidth="60"/>
        <ListView fx:id="to"  HBox.hgrow="NEVER" prefHeight="300"/>
    </HBox>
</UIView>